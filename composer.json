{
  "name": "luoyue/webman-mcp",
  "type": "library",
  "license": "MIT",
  "description": "Webman plugin luoyue/webman-mcp",
  "require": {
    "php": ">=8.1",
    "mcp/sdk": "~v0.2.0|dev-main",
    "nyholm/psr7": "^1.8",
    "webman/console": "^2.1",
    "webman/cache": "^2.1"
  },
  "require-dev": {
    "workerman/webman-framework": "^2.1",
    "monolog/monolog": "^3.9",
    "mcp/sdk": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "Luoyue\\WebmanMcp\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
}
